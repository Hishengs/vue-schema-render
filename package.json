{
  "name": "vue-schema-render",
  "version": "0.1.2",
  "description": "A Form Render Based on JSON Schema for Vue",
  "author": "hishengs@gmail.com",
  "license": "MIT",
  "keywords": [
    "vue",
    "schema",
    "render"
  ],
  "repository": "https://github.com/Hishengs/vue-schema-render",
  "main": "dist/vue-schema-render.common.js",
  "files": [
    "dist",
    "src"
  ],
  "typings": "src/types.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack-dev.config.js --open",
    "build:common": "webpack --config build/webpack-prod.config.js",
    "build:umd": "webpack --config build/webpack-umd.config.js",
    "dist": "npm run build:common && npm run build:umd",
    "storybook": "start-storybook -p 7666 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "build": "cross-env NODE_ENV=production npm run dist && npm run build-storybook",
    "docs": "vuepress dev ./docs --open",
    "test": "cross-env NODE_ENV=test jest -i --updateSnapshot"
  },
